{
  "expo": {
    "name": "Rent Easy 9ja",
    "slug": "RentEasy9ja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF",
      "dark": {
        "image": "./assets/splash-dark.png",
        "backgroundColor": "#000000"
      },
      "imageWidth": 200
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "5",
      "supportsTablet": true,
      "bundleIdentifier": "com.RentEasy9ja.org",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.RentEasy9ja.org"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [],
    "extra": {
      "API_BASE_URL": "http://localhost:3000/api",
      "API_BASE_URL_PROD": "https://rent-easy-9ja-api.onrender.com/api",
      "eas": {
        "projectId": "24757c29-3e2f-48eb-bdc1-063e574a7bc8"
      }
    }
  }
}
